.progress-donut
  position relative
  float left

.progress-donut__text
  text-align center
  position absolute
  line-height 150px
  width 100%

.progress-donut__number
  font-size 30px
  padding-left 3px
  padding-right 3px

.progress-donut__of
  font-size 14px

.progress-donut svg
  -webkit-transform rotate(-90deg)
  transform rotate(-90deg)

.progress-donut__complete
  stroke-dasharray 440
  stroke-dashoffset 80
